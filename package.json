{
  "name": "browserslist-new-relic",
  "version": "1.0.0-rc.1",
  "description": "A CLI tool that generates a custom usage data file for browserslist from your New Relic Browser data.",
  "bin": {
    "browserslist-new-relic": "index.js"
  },
  "scripts": {
    "build": "rm -rf ./dist && mkdirp ./dist/lib && cp -a ./lib/*.js ./dist/lib/ && cp -a ./index.js ./dist/ && cp -a ./package.json ./dist/ && cp -a ./LICENSE ./dist/ && cp -a ./README.md ./dist/",
    "release": "npm run release-prep && release-it --npm.publishPath=./dist/",
    "stage:major": "release-it major --ci --no-npm --github.release",
    "stage:minor": "release-it minor --ci --no-npm --github.release",
    "stage:patch": "release-it patch --ci --no-npm --github.release",
    "stage:rc": "release-it --ci --preRelease=rc --npm.tag=next --no-npm.publish --github.release",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syntactic-salt/browserslist-new-relic.git"
  },
  "keywords": [
    "browserslist-new-relic",
    "browserslist",
    "browserslist-stats.json",
    "new relic"
  ],
  "author": "Ken Pereira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/syntactic-salt/browserslist-new-relic/issues"
  },
  "homepage": "https://github.com/syntactic-salt/browserslist-new-relic#readme",
  "dependencies": {
    "command-line-args": "5.1.1",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "jest": "26.6.3",
    "mkdirp": "1.0.4",
    "release-it": "14.2.2"
  }
}
